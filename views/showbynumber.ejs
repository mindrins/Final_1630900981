<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Check Lottery By number</title>
</head>
<body>
    <table>
        <tr>
            <th>เลขรางวัล</th>
            <th>รอบวันที่</th>
            <th>เดือน</th>
            <th>รางวัล</th>
        </tr>
        <% lottery.forEach(function(item) { %>
            <tr>
                <td><%= item.number %></td>
                <td><%= item.day %></td>
                <td><%= item.month %></td>
                <td><%= item.prize %></td>
                
            </tr>
            <% }) %>
    </table>
</body>
</html>